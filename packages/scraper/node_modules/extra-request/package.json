{
  "name": "extra-request",
  "version": "10.1.0",
  "description": "Utilities for Request",
  "type": "module",
  "sideEffects": false,
  "keywords": [
    "fetch",
    "Request"
  ],
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./transformers": {
      "types": "./lib/transformers/index.d.ts",
      "default": "./lib/transformers/index.js"
    },
    "./transformers/*": {
      "types": "./lib/transformers/*.d.ts",
      "default": "./lib/transformers/*.js"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "repository": "git@github.com:BlackGlory/extra-request.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --quiet src __tests__",
    "test": "vitest --run",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@types/papaparse": "^5.3.16",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "extra-fetch": "^5.0.1",
    "extra-filesystem": "^0.5.2",
    "husky": "4",
    "npm-run-all": "^4.1.5",
    "rimraf": "6.0.1",
    "standard-version": "^9.5.0",
    "ts-patch": "^3.3.0",
    "tslib": "^2.8.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.34.0",
    "typescript-transform-paths": "^3.5.5",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.4.0",
    "extra-utils": "^5.19.0",
    "js-base64": "^3.7.7",
    "justypes": "^4.4.1",
    "papaparse": "^5.5.3",
    "url-operator": "^0.3.1"
  },
  "peerDependencies": {
    "extra-fetch": "^5.0.1"
  }
}
