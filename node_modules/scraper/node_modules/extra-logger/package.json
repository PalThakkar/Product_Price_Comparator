    "rimraf": "^5.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-patch": "^3.0.1",
    "tslib": "^2.6.0",
    "typescript": "5.1.6",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.3.3",
    "chalk": "^5.3.0"
  }
